{
	"name": "Refind",
	"version": "0.1.7",
	"description": "Fast on-page finding and clicking.",
	"manifest_version": 2,
	"icons": {
		"16"	: "icon16.png",
		"48"	: "icon48.png",
		"128" : "icon128.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["refind.css"],
			"js": ["refind.js"]
		}
	]
}
